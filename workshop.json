	    }
	},
	{
	    "name": "python-jsonschema",
	    "type": "manual",
	    "manual_info": {
		"commands": [
		    "pip3 install jsonschema"
		]
	    }
	},
	{
	    "name": "python3-redis",
	    "type": "distro",
	    "distro_info": {
		"packages": [
		    "python3-redis"
		]
	    }
	},
	{
	    "name": "python3-jsonschema",
	    "type": "distro",
	    "distro_info": {
		"packages": [
		    "python3-jsonschema"
		]
	    }
	}
    ]
