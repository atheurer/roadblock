{
    "workshop": {
        "schema": {
            "version": "2020.03.02"
        }
    },
    "userenvs": [
	{
	    "name": "default",
	    "requirements": [
		"python36",
		"python-redis",
		"python-jsonschema"
	    ]
	},
	{
	    "name": "opensuse",
	    "requirements": [
		"python3",
		"python-redis",
		"python-jsonschema"
	    ]
    },
	{
	    "name": "debian",
	    "requirements": [
		"python3",
		"python-redis",
		"python-jsonschema"
	    ]
    },
	{
	    "name": "fedora31",
	    "requirements": [
		"python36",
		"python3-redis",
		"python3-jsonschema"
	    ]
	}
    ],
    "requirements": [
	{
	    "name": "python36",
